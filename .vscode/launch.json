{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch zinseszins with GDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/zinseszins/zinseszins",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "link-asm"
        }
    ]
}
